# AI-NoteBook 环境变量配置

# ================================
# 数据库配置
# ================================
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/ainotebook"

# ================================
# Redis 配置
# ================================
REDIS_URL="redis://localhost:6379"

# ================================
# JWT 配置
# ================================
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"
JWT_EXPIRES_IN="7d"

# ================================
# Volcengine AI 配置
# ================================
VOLCENGINE_API_KEY="your-volcengine-api-key"
VOLCENGINE_MODEL="glm-4.7"
VOLCENGINE_ENDPOINT="https://ark.cn-beijing.volces.com/api/v3"

# ================================
# 应用配置
# ================================
NODE_ENV="development"
API_PORT=3000
FRONTEND_PORT=5173

# ================================
# 限制配置
# ================================
MAX_FREE_ANALYSES=100
MAX_CONTENT_LENGTH=10000
MAX_FILE_SIZE_MB=5

# ================================
# 队列配置
# ================================
QUEUE_CONCURRENCY=5
QUEUE_MAX_RETRIES=3
